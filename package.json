{
  "name": "iterates",
  "description": "Iterator and AsyncIterator helper functions with typings",
  "version": "0.3.1",
  "author": "Rasmus Eneman <rasmus@eneman.eu>",
  "repository": "Pajn/iterates",
  "homepage": "https://pajn.github.io/iterates",
  "keywords": [
    "iterator",
    "asynciterator",
    "async",
    "typescript"
  ],
  "dependencies": {
    "auto-curry": "^0.2.1",
    "babel-runtime": "^6.26.0"
  },
  "devDependencies": {
    "@types/jest": "^20.0.4",
    "coveralls": "^2.13.1",
    "documittu": "^0.3.0",
    "documittu-analyzer-ts": "^0.3.0",
    "documittu-template-default": "^0.3.0",
    "prettier": "^1.5.3",
    "tscomp": "^0.13.2",
    "tslint": "^5.5.0",
    "tslint-config-pajn": "^1.0.0"
  },
  "tscomp": {
    "mode": "lib"
  },
  "scripts": {
    "build": "tscomp build",
    "test": "tscomp test",
    "eject": "tscomp eject",
    "watch": "tscomp watch",
    "prepack": "tscomp build",
    "format": "prettier --write --single-quote --trailing-comma all --no-bracket-spacing --no-semi src/**/*.{js,jsx,ts,tsx}",
    "report-coverage": "cat coverage/lcov.info | coveralls",
    "docs": "yarn docs-analyze && documittu build",
    "docs-dev": "documittu start",
    "docs-analyze": "documittu-analyzer-ts . > docs/analyze-result.json"
  },
  "main": "./cjs/index.js",
  "module": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "license": "BSD-3-Clause"
}
