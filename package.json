{
  "name": "iterates",
  "description": "Iterator and AsyncIterator helper functions with typings",
  "version": "1.2.2",
  "author": "Rasmus Eneman <rasmus@eneman.eu>",
  "repository": {
    "type": "git",
    "url": "git@github.com:Pajn/iterates.git"
  },
  "homepage": "https://pajn.github.io/iterates",
  "keywords": [
    "iterator",
    "asynciterator",
    "async",
    "typescript"
  ],
  "sideEffects": false,
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "@types/node": "^13.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@types/jest": "^25.1.1",
    "coveralls": "^3.0.9",
    "prettier": "^1.19.1",
    "tslint": "^5.11.0",
    "tslint-config-pajn": "^1.0.0"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "8.0.5"
  },
  "tscomp": {
    "mode": "lib"
  },
  "scripts": {
    "build": "./node_modules/tsmodule/bin/tsmodule.js build",
    "test": "tscomp test",
    "eject": "tscomp eject",
    "watch": "tscomp watch",
    "prepack": "yarn build",
    "format": "prettier --write --single-quote --trailing-comma all --no-bracket-spacing --no-semi src/**/*.{js,jsx,ts,tsx}",
    "report-coverage": "cat coverage/lcov.info | coveralls"
  },
  "main": "./cjs/index.js",
  "module": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "license": "BSD-3-Clause"
}
