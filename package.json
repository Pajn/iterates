{
  "name": "iterates",
  "description": "Iterator and AsyncIterator helper functions with typings",
  "version": "1.1.0-beta",
  "author": "Rasmus Eneman <rasmus@eneman.eu>",
  "repository": {
    "type": "git",
    "url": "git@github.com:Pajn/iterates.git"
  },
  "homepage": "https://pajn.github.io/iterates",
  "keywords": [
    "iterator",
    "asynciterator",
    "async",
    "typescript"
  ],
  "sideEffects": false,
  "dependencies": {
    "@babel/runtime": "^7.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@types/jest": "^24.0.9",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "coveralls": "^2.13.1",
    "prettier": "^1.15.3",
    "tscomp": "^1.0.0-beta.10",
    "tslint": "^5.11.0",
    "tslint-config-pajn": "^1.0.0"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "tscomp": {
    "mode": "lib"
  },
  "scripts": {
    "build": "yarn build:src && yarn build:mark-pure",
    "build:src": "tscomp build",
    "build:mark-pure": "babel --plugins annotate-pure-calls lib/* -d lib",
    "test": "tscomp test",
    "eject": "tscomp eject",
    "watch": "tscomp watch",
    "prepack": "yarn build",
    "format": "prettier --write --single-quote --trailing-comma all --no-bracket-spacing --no-semi src/**/*.{js,jsx,ts,tsx}",
    "report-coverage": "cat coverage/lcov.info | coveralls"
  },
  "main": "./cjs/index.js",
  "module": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "license": "BSD-3-Clause"
}
