{
  "name": "iterates",
  "description": "Iterator and AsyncIterator helper functions with typings",
  "version": "2.0.0-beta.1",
  "author": "Rasmus Eneman <rasmus@eneman.eu>",
  "repository": {
    "type": "git",
    "url": "git@github.com:Pajn/iterates.git"
  },
  "homepage": "https://pajn.github.io/iterates",
  "keywords": [
    "iterator",
    "asynciterator",
    "async",
    "typescript"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.19",
    "babel-jest": "^26.6.3",
    "coveralls": "^2.13.3",
    "jest": "^26.6.3",
    "prettier": "^1.19.1",
    "ts-node": "^9.1.1",
    "tslint": "^5.11.0",
    "tslint-config-pajn": "^1.0.0",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "yarn build::esm && yarn build::cjs",
    "build::esm": "tsc",
    "build::cjs": "tsc --module commonjs --outDir cjs",
    "test": "jest",
    "prepack": "yarn build",
    "format": "prettier --write --single-quote --trailing-comma all --no-bracket-spacing --no-semi src/**/*.{js,jsx,ts,tsx}",
    "report-coverage": "cat coverage/lcov.info | coveralls"
  },
  "main": "./cjs/index.js",
  "module": "./lib/index.js",
  "export": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "license": "BSD-3-Clause"
}
