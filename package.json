{
  "name": "iterates",
  "description": "Iterator and AsyncIterator helper functions with typings",
  "version": "0.3.8",
  "author": "Rasmus Eneman <rasmus@eneman.eu>",
  "repository": "Pajn/iterates",
  "homepage": "https://pajn.github.io/iterates",
  "keywords": [
    "iterator",
    "asynciterator",
    "async",
    "typescript"
  ],
  "sideEffects": false,
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "auto-curry": "^0.2.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "coveralls": "^2.13.1",
    "prettier": "^1.15.2",
    "tscomp": "^1.0.0-beta.9",
    "tslint": "^5.11.0",
    "tslint-config-pajn": "^1.0.0"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "tscomp": {
    "mode": "lib"
  },
  "scripts": {
    "build": "tscomp build",
    "test": "tscomp test",
    "eject": "tscomp eject",
    "watch": "tscomp watch",
    "prepack": "tscomp build",
    "format": "prettier --write --single-quote --trailing-comma all --no-bracket-spacing --no-semi src/**/*.{js,jsx,ts,tsx}",
    "report-coverage": "cat coverage/lcov.info | coveralls"
  },
  "main": "./cjs/index.js",
  "module": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "license": "BSD-3-Clause"
}
